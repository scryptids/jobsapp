{
  "name": "www",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "codegen-compile": "node -r dotenv/config node_modules/.bin/gql-gen",
    "codegen-watch": "node -r dotenv/config node_modules/.bin/gql-gen -w",
    "prestart": "npm run codegen-compile",
    "predev": "npm run codegen-compile",
    "test": "vitest",
    "gen-hasura-jwt": "ts-node --project tsconfig.alt.json -r tsconfig-paths/register \"./tools/gen-hasura-jwt.js\"",
    "gen-v4-uuid": "ts-node --project tsconfig.alt.json -r tsconfig-paths/register \"./tools/gen-v4-uuid.js\""
  },
  "dependencies": {
    "@mantine/core": "^8.0.1",
    "@mantine/hooks": "^8.0.1",
    "@react-router/fs-routes": "^7.6.0",
    "@react-router/node": "^7.6.0",
    "@react-router/serve": "^7.6.0",
    "@tabler/icons-react": "^3.11.0",
    "@tanstack/react-form": "^1.11.2",
    "@tanstack/react-table": "^8.19.3",
    "dotenv": "^16.4.5",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "isbot": "^5.1.28",
    "jose": "^6.0.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.6.0",
    "uuid": "^11.1.0",
    "xstate": "^5.13.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@react-router/dev": "^7.6.0",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "8.56",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.38",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.1.3"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
